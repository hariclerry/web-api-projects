{
  "name": "Battery Monitor",
  "author": "Harriet",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
"permissions": ["tabs", "scripting"],
"content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "images/icon-48.png"
  }
}